{
  "name": "generator-ts-normalization",
  "version": "0.0.1",
  "description": "Add typescript, eslint, prettier and editorconfig to your project!",
  "homepage": "",
  "author": {
    "name": "Mirone",
    "email": "Saul-Mirone@outlook.com",
    "url": "https://saul-mirone.github.io"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "prettier",
    "typescript",
    "eslint",
    "editorconfig",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "3.1.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-config-xo": "0.40.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^28.0.0",
    "lint-staged": "^12.0.0",
    "prettier": "^2.0.0",
    "typescript": "4.6.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^6.0.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "yeoman-generator": "^5.0.0",
    "chalk": "^5.0.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saul-Mirone/generator-ts-normalization.git"
  },
  "license": "MIT"
}
